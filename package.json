{
  "name": "enos-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "ci:travis": "npm run lint && npm run test:ci && npm run build:aot && npm run e2e:travis",
    "test:ci": "karma start --single-run --browsers ChromeTravisCi",
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 SOURCE_MAP=0 npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "e2e:travis": "npm-run-all -p -r server:prod:ci protractor:delay",
    "protractor": "protractor",
    "protractor:delay": "sleep 3 && npm run protractor",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "clean:aot": "npm run rimraf -- compiled",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "node-sass": "^4.9.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.2",
    "@angular-devkit/build-angular": "~0.6.3",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.3",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cross-env": "^5.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "rimraf": "~2.6.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-loader": "^3.5.3"
  }
}
